@model List<Exam_system.UI.Models.StudentSubjects>
@{ 
    ViewBag.Title = "My Subjects";
}

<table style="margin-top: 50px;" class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>Subject Name</th>
            <th>Subject Creation Date</th>
            <th>Your Grade</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
        <tr>
            <td>@item.Subject.Name</td>
            <td>@item.Subject.CreationDate</td>
            <td>@item.Grade</td>
        </tr>
        }
    </tbody>
</table>